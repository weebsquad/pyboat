{
  "name": "pyboat",
  "version": "1.7.7",
  "description": "yes",
  "scripts": {
    "typings": "typings install",
    "build": "npm run-script typings && npm run-script updateglobalversion && rollup -c rollup.config.js",
    "publish": "node cd/publish.js",
    "lint": "eslint cd/publish.js --config .eslintrc.js && eslint src/ --ext .ts --max-warnings 99999 --config .eslintrc.ts",
    "lintfix": "eslint cd/publish.js --config .eslintrc.js --quiet --fix && eslint src/ --ext .ts --fix --config .eslintrc.ts --quiet",
    "test": "npm run-script i18ntypings && rollup -c rollup_test.config.js && npm run-script publish",
    "testlint": "npm run-script i18ntypings && npm run-script lintfix && rollup -c rollup_test.config.js && npm run-script publish",
    "i18ntypings": "node cd/i18n_typings.js",
    "updateglobalversion": "node cd/globalversion.js"
  },
  "author": "weebsquad",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/node": "^15.0.2",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "json-ts": "^1.6.4",
    "mocha": "^9.0.0",
    "node-fetch": "^2.6.0",
    "rollup": "2.52.0",
    "rollup-plugin-ignore": "^1.0.6",
    "rollup-plugin-terser": "^7.0.0",
    "typescript": "^4.0.5",
    "typescript-tools": "^0.3.1",
    "typings": "^2.1.1",
    "ws": "^7.2.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.8.0",
    "imagescript": "^1.0.7",
    "install": "^0.13.0",
    "js-yaml": "^4.0.0",
    "npm": "^7.5.6",
    "path": "^0.12.7",
    "sleep": "^6.3.0",
    "typescript-tslint-plugin": "^1.0.0"
  }
}
