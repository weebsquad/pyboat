{
  "name": "pyboat",
  "version": "1.0.0",
  "description": "yes",
  "scripts": {
    "typings": "typings install",
    "build": "npm run-script typings && rollup -c rollup.config.js",
    "publish": "node cd/publish.js",
    "lint": "eslint cd/publish.js --config .eslintrc.js && eslint src/ --ext .ts --max-warnings 99999 --config .eslintrc.ts",
    "lintfix": "eslint cd/publish.js --config .eslintrc.js --quiet --fix && eslint src/ --ext .ts --fix --config .eslintrc.ts --quiet",
    "test": "rollup -c rollup.config.js && npm run-script publish",
    "testlint": "npm run-script lintfix && rollup -c rollup.config.js && npm run-script publish"
  },
  "author": "weebsquad",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^8.1.1",
    "node-fetch": "^2.6.0",
    "rollup": "^2.6.1",
    "rollup-plugin-ignore": "^1.0.6",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.28.0",
    "typescript": "^4.0.5",
    "typescript-tools": "^0.3.1",
    "ws": "^7.2.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.8.0",
    "install": "^0.13.0",
    "js-yaml": "^3.14.0",
    "npm": "^6.14.7",
    "path": "^0.12.7",
    "sleep": "^6.3.0",
    "typescript-tslint-plugin": "^0.5.5",
    "typings": "^2.1.1"
  }
}
